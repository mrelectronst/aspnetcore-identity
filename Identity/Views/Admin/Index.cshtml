@model List<AppUser>

@{
    ViewData["Title"] = "Admin Page";
}

<table class="table table-bordered table-striped">

    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
    </tr>

    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan=3>Users Not Found</td>
        </tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    }
</table>